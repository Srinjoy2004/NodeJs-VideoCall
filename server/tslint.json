{
    "defaultSeverity": "error",
    "extends": "tslint:recommended",
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "max-line-length": [
            true,
            100
        ],
        "semicolon": {
            "options": [
                "always"
            ]
        },
        "eofline": false
    },
    "jsRules": {},
    "rulesDirectory": []
}